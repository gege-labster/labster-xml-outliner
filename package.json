{
	"name": "labster-xml-outliner",
	"displayName": "Labster XML Outliner",
	"description": "Outline XML, duh",
	"publisher": "gegeturtle",
	"version": "0.0.1",
	"repository": "https://github.com/gege-labster/labster-xml-outliner/",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:xml",
		"onView:labsterXMLOutline"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "labster-xml-outliner.refresh",
				"title": "Refresh"
			},
			{
				"command": "labster-xml-outliner.gotoline",
				"title": "Select Line"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "labsterXMLOutline",
					"name": "Labster engine.xml outline",
					"when": "fileIsXML"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"xml2js": "^0.4.23"
	},
	"dependencies": {
		"@types/node": "^16.11.11",
		"@types/vscode": "^1.62.0",
		"@types/xmldom": "^0.1.31",
		"fast-xml-parser": "^4.0.0-beta.3",
		"path": "^0.12.7",
		"xmldom": "^0.6.0"
	}
}
